molecule
{
	angstrom2bohr?
		double 1.88972612456506198632428439,
	units?
		enum { angstrom, bohr },
	coords?
		enum { internal, cartesian },
	multiplicity?
		int 1,
	charge?
		int 0,
	atom+
	{
		basis_set? string,
		truncation? string,
		# ZMAT specification, e.g. H 2 R 1 A
		# or xyz position
		*+
	},
	basis?
	{
		contaminants?
			bool false,
		spherical?
			bool true,
		basis_set? string,
		truncation*
		{
			# elements affected, e.g. H-Ne
			*,
			# same format as molecule.atom.truncation
			*
		}
	}
},
